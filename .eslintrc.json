{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "no-bitwise": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    },
    "root": true
}
